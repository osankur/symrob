	trans: tau { LP2counter_0__out = (1-((1-((1-((LP2counter_0__out) * (LP2n7))) * (1-(IP2stay)))) * (1-(((LP2counter_0__out) * (LP2n7)) * (IP2stay))))); } -&gt;UpdatedLP2counter_0__out;
	endstate

	state UpdatedLP2counter_0__out	
