var out8 : {0..2} = 2;
var out9 : {0..2} = 2;
System = Node0()||| Node1()||| Node2()||| Node3()||| Node4()||| Node5()||| Inputs();
#define Err err==1;
#assert System reaches Err;

TimedAutomaton Node0
{
clock: t, x0;

	state down	
	inv: x0 &lt;= 100;
	trans: [[100 &lt;= x0 &amp;&amp; x0 &lt;= 200 &amp;&amp; t &lt;= 1000]] [out5 &lt; 2 &amp;&amp; out4 &lt; 2 &amp;&amp; out5 == 1 &amp;&amp; out4 == 1] tau { out0 = 1 } -&gt;up;
	trans: [[t &gt;= 1001]] tau { out0 = 2;out1 = 2;out2 = 2;out3 = 2;out4 = 2;out5 = 2 } -&gt;down;
	trans: [[100 &lt;= x0 &amp;&amp; x0 &lt;= 200 ]] [out5 &lt; 2 &amp;&amp; out4 &lt; 2 &amp;&amp; !(out5 == 1 &amp;&amp; out4 == 1)] tau { out0 = 0 } -&gt;up;
	trans: [[x0 == 100 ]] [(out5 != 2 &amp;&amp; out4 != 2) == 0] tau &lt;x0&gt; -&gt;down;
	endstate

	state up	
	inv: x0 &lt;= 200;
	trans: [[x0 &lt;=200]] tau { out0 = 2 } &lt;x0&gt; -&gt;down;
	trans: [out0==1] tau { err = 1; } -&gt;err;
	endstate

	state err	
	endstate

}
TimedAutomaton Node1
{
clock: x1;

	state down	
	inv: x1 &lt;= 50;
	trans: [[50 &lt;= x1 &amp;&amp; x1 &lt;= 100 ]] [out6 &lt; 2 &amp;&amp; out7 &lt; 2 &amp;&amp; out8 &lt; 2 &amp;&amp; out9 &lt; 2 &amp;&amp; (out6+(out7+(out8+(out9)))) &gt;= 3] tau { out1 = 1 } -&gt;up;
	trans: [[50 &lt;= x1 &amp;&amp; x1 &lt;= 100 ]] [out6 &lt; 2 &amp;&amp; out7 &lt; 2 &amp;&amp; out8 &lt; 2 &amp;&amp; out9 &lt; 2 &amp;&amp; (out6+(out7+(out8+(out9)))) &lt; 3] tau { out1 = 0 } -&gt;up;
	trans: [[x1 == 50 ]] [(out6 != 2 &amp;&amp; (out7 != 2 &amp;&amp; (out8 != 2 &amp;&amp; (out9 != 2)))) == 0] tau &lt;x1&gt; -&gt;down;
	endstate

	state up	
	inv: x1 &lt;= 100;
	trans: [[x1 &lt;=100]] tau { out1 = 2 } &lt;x1&gt; -&gt;down;
	endstate

}
