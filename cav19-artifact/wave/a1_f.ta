<TA><Declaration> var err : {0..1} = 0;
var out0 : {0..2} = 2;
var b0_0 : {0..2} = 2;
var b0_1 : {0..2} = 2;
var out1 : {0..2} = 2;
var b1_0 : {0..2} = 2;
var b1_1 : {0..2} = 2;
var b1_2 : {0..2} = 2;
var b1_3 : {0..2} = 2;
