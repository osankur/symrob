TimedAutomaton Inputs
{

	state input_init	
	committed;
	trans: tau { out6 = 1 } -&gt;In6;
	trans: tau { out6 = 0 } -&gt;In6;
	endstate

	state In6	
	committed;
	trans: tau { out7 = 1 } -&gt;In7;
